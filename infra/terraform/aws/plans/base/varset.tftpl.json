{
  "data": {
    "type": "varsets",
    "attributes": {
      "name": "project-${name_prefix}",
      "description": "workspace varset ",
      "global": false,
      "priority": false
    },
    "relationships": {
      "projects": {
        "data": [
          {
            "id": "${terraform_cloud_prj_id}",
            "type": "projects"
          }
        ]
      },
      "vars": {
        "data": [
          {
            "type": "vars",
            "attributes": {
              "key": "vpc_id",
              "value": "${vpc_id}",
              "category": "terraform"
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "vpc_cidr",
              "value": "${vpc_cidr}",
              "category": "terraform"
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "vpc_private_subnet_ids",
              "value": "${vpc_private_subnet_ids}",
              "category": "terraform",
              "hcl":true
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "vpc_public_subnet_ids",
              "value": "${vpc_public_subnet_ids}",
              "category": "terraform",
              "hcl":true
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "keypair_name",
              "value": "${keypair_name}",
              "category": "terraform"
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "vpc_allowlist_sg_id",
              "value": "${vpc_allowlist_sg_id}",
              "category": "terraform"
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "base_tags_json",
              "value": "${base_tags_json}",
              "category": "terraform"
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "isp_sub_domain",
              "value": "${isp_sub_domain}",
              "category": "terraform"
            }
          },
          {
            "type": "vars",
            "attributes": {
              "key": "terraform_cloud_prj_id",
              "value": "${terraform_cloud_prj_id}",
              "category": "terraform"
            }
          }
        ]
      }
    }
  }
}
